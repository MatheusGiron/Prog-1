#include <stdio.h>

int main()

{  int n,i,ant=0,pos=1;

   printf("Digite ate qual numero deseja imprimir a sequencia de Fibonacci: ");
   scanf("%d", &n);

   for (i=3; ant<=n; i++){
    printf("%d, ", ant);

   if(pos<=n){
        printf("%d, ", pos);}

    ant=pos+ant;
    pos=pos+ant;}

return 0;}
