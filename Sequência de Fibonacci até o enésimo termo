#include <stdio.h>

int main()

{  int n,i,ant=0,pos=1;

   printf("Digite ate qual termo deseja imprimir a sequencia de Fibonacci: ");
   scanf("%d", &n);

   for (i=0; i<n; i++){
    printf("%d, ", ant);i++;

   if(i<n){
        printf("%d, ", pos);}

    ant=pos+ant;
    pos=pos+ant;}

return 0;}
